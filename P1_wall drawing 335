var x3=200;
var x4=300;
var y3=width/2;
var y4=width/2;

while(y3<width/2+50){
  line(x3,y3,x4,y4);
  x3=x3+5;
  x4=x4-5;
  y3=y3-10;
  y4=y4-10;
}
  
