colorMode(HSB);
var x=0;
var y=0;

function squares(){
fill(0,0,random(10,80));
rect(x,y,50,50);
fill(0,0,random(10,80));
rect(x+55,y,50,50);
fill(0,0,random(10,80));
rect(x+55,y+55,50,50);
fill(0,0,random(10,80));
rect(x,y+55,50,50);
}




for(var x=0; x< width-55; x+=115){
		for(var y=0; y<height-100; y+=115){
 		squares();
    }
    }
