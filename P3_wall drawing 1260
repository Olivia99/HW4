rectMode(CENTER);
rect(width/2,height/2,200,200,10,10,10,10);


var t=100;
var l=150;
var time=0
while(time<6){
  
fill(255,255,255,t);
rect(width/2,height/2,l,l,10,10,10,10);
t=t-10;
l=l+7;
  time =time+1
}
var time=0
var t=100;
var l=100;
while(time<9){
noStroke();
fill(0,0,0,t);
rect(width/2,height/2,l,l,10,10,10,10);
t=t-7;
l=l+5;
  time =time+1
}

fill(255);
rect(width/2,height/2,100,100,10,10,10,10);
